<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CampoMain">

        <attr name="clnSqlNome" format="string"/>
        <attr name="booSomenteLeitura" default="false" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="LabelGeral">

        <attr name="enmFonteTamanho" format="enum">
            <enum name="GRANDE" value="4"/>
            <enum name="MEDIO" value="3"/>
            <enum name="MINIM0" value="0"/>
            <enum name="NORMAL" value="2"/>
            <enum name="PEQUENO" value="1"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="LinhaGeral">

        <attr name="enmDisposicao" format="enum">

            <enum name="HORIZONTAL" value="0"/>
            <enum name="VERTICAL" value="1"/>

        </attr>

    </declare-styleable>

    <declare-styleable name="MenuItem">

        <attr name="srcIcone" format="integer"/>
        <attr name="strTituloMenu" default="Menu desconhecido" format="string"/>

    </declare-styleable>

    <declare-styleable name="PainelLinha">

        <attr name="intNivelQuantidade" format="integer"/>

    </declare-styleable>

    <declare-styleable name="View">

        <attr name="strTitulo" format="string"/>

    </declare-styleable>

</resources>